section.content
	.content-box
		.video-show
			h3 !{video.title}
			.video-a
				embed(src='!{video.src}' allowFullScreen='true' quality='high' width='860' height='524' align='middle' allowScriptAccess='always' type='application/x-shockwave-flash')
